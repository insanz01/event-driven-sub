{
  "name": "tuv-nord-central-api",
  "version": "1.0.0",
  "description": "HSD Template for Express API with Prisma",
  "main": "src/index.js",
  "prisma": {
    "seed": "ts-node prisma/seeder/seed.ts"
  },
  "scripts": {
    "build": "rm -rf dist && npx tsc && cp -r src/prisma dist/prisma",
    "start": "node dist/index.js",
    "dev": "nodemon --exec 'node -r @swc-node/register' src/index.ts",
    "type-check": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "db.migrate": "npx prisma migrate dev",
    "db.generate": "npx prisma generate",
    "db.seed": "npx prisma db seed",
    "db.push": "npx prisma db push",
    "db.pull": "npx prisma db pull",
    "deploy:dev": "gcloud app deploy api-dev.yaml --project=tuvnordcentral"
  },
  "keywords": [],
  "author": "Prawito Hudoro",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "amqplib": "^0.10.3",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "express-rate-limit": "^7.2.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "short-unique-id": "^5.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@swc-node/register": "^1.9.0",
    "@swc/core": "^1.4.11",
    "@types/amqplib": "^0.10.5",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mime": "^4.0.0",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-config-love": "^43.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.4",
    "prisma": "^5.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
